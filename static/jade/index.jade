    script.
      $(function () {
        var
          lng = 16.596936,
          lat = 49.226245;

        var map = new GMaps({
          div: '#map',
          zoom: 17,
          lat: lat,
          lng: lng,
          scrollwheel: false
        });

        map.addMarker({
          lat: lat,
          lng: lng,
          title: 'PyCon CZ 2015'
        });

        $('body').scrollspy({target: '#navbar-example'})
      });
