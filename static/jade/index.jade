- var contactEmail = "tomas.ehrlich@gmail.com"


doctype html
html(lang='en-us')
  head
    include ./components/head
  body.home(id='', data-spy='scroll', data-target='#navbar-example')
    div
      header
        .navbar.navbar-default.navbar-fixed-top#navbar-example
          ul.nav.navbar-nav
            li
              a(href='#section-sponsors') section-sponsors
            li
              a(href='#section-speakers') speakers
      .jumbotron(xmlns='http://www.w3.org/1999/html')
        .container
          img.python-logo(src='/static/images/psf/python-notext.png')
          h1
            span.tri1 PyCon
            span.tri2 C
            span.tri3 Z
            |  2015
          .row
            .col-md-3.col-md-offset-3
              p.event-location Brno, Czech Republic
            .col-md-3
              p.event-date 14-15. November 2015
      .claim
        .container
          p.lead
            | The very first Python conference in Czech republic.
            | Join us for one day of talks about Python and Python related topics.
      section#section-sponsors.sponsors
        include ./pages/sponsors
      .soon
        .container
          p.lead More info coming soon, ETA end of June 2015
      section#section-speakers.speakers
        include ./pages/speakers
      section#section-map.map
        include ./pages/map
      hr
      footer
        .container
          footer
            a(href='https://github.com/tricoder42/cz.pycon.org-2015/blob/feature/code-of-conduct/code-of-conduct.md')
              | Code
              | of conduct
            p.psf-trademark
              a(href='https://www.python.org/psf/trademarks/pycon/') PyConâ„¢
              |     is a trademark for worldwide conference activities claimed by
              a(href='https://www.python.org/psf-landing/') Python software foundation
              | .

